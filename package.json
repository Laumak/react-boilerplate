{
  "name": "react-boilerplate",
  "version": "1.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "predev": "babel-node buildScripts/devMessage.js",
    "dev": "npm-run-all --parallel open:src lint:watch start-mockapi",
    "open:src": "babel-node buildScripts/devServer.js",
    "lint": "esw src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel prod localtunnel",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "preprod": "npm-run-all clean-dist lint",
    "prod": "babel-node buildScripts/build.js",
    "postprod": "babel-node buildScripts/prodServer.js",
    "generate-mock-data": "babel-node buildScripts/generateMockData.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001"
  },
  "dependencies": {
    "bulma": "^0.3.1",
    "font-awesome": "^4.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.24.1",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "node-sass": "^4.1.1",
    "npm-run-all": "^3.1.2",
    "open": "^0.0.5",
    "postcss-loader": "^1.2.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
